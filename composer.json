{
    "name": "fungku/kwiki",
    "description": "Quick wiki or blog with markdown files",
    "keywords": [
        "wiki",
        "blog",
        "markdown"
    ],
    "license": "Apache-2.0",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "ryanwinchester/pagemark": "^0.6",
        "laravel/framework": "5.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0",
        "laravel/homestead": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Fungku\\Kwiki\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}